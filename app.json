{
  "expo": {
    "name": "yhiw",
    "slug": "yhiw",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yhiw",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yhiw.app"
    },

    "android": {
      "package": "com.yhiw.app",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ],

    "extra": {
      "eas": {
        "projectId": "95f70f6d-e9da-4a6d-a5cb-dc10e9d91005"
      }
    }
  }
}